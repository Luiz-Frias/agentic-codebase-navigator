{
  "$schema": "https://raw.githubusercontent.com/DetachHead/basedpyright/main/packages/pyright/schemas/pyrightconfig.schema.json",
  "include": ["src/rlm/domain", "src/rlm/application"],
  "exclude": ["src/rlm/_legacy", "references", "tests", "**/__pycache__"],
  "pythonVersion": "3.12",
  "typeCheckingMode": "standard",
  "reportMissingImports": false,
  "reportMissingTypeStubs": false,
  "reportAny": true,
  "reportUnusedImport": false,
  "reportUnusedVariable": false
}

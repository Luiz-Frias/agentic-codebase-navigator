{
  "lineWidth": 100,
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.17.8.wasm"
  ],
  "markdown": {
    "lineWidth": 100,
    "textWrap": "maintain"
  },
  "includes": [
    "**/*.md"
  ],
  "excludes": [
    "**/node_modules",
    "**/.git",
    "**/dist",
    "**/build",
    "**/.venv",
    "**/CHANGELOG.md",
    "**/LICENSE.md",
    "**/docs/**"
  ]
}
